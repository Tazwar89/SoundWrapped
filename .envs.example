# SoundCloud API Configuration
SOUNDCLOUD_CLIENT_ID=your_soundcloud_client_id_here
SOUNDCLOUD_CLIENT_SECRET=your_soundcloud_client_secret_here
REDIRECT_URI=http://localhost:8080/callback

# Database Configuration
POSTGRES_DB=soundwrapped_db
POSTGRES_USER=soundwrapped_user
POSTGRES_PASSWORD=supersecretpassword

# Spring Boot Configuration (PostgreSQL)
SPRING_PROFILES_ACTIVE=default
SPRING_DATASOURCE_URL=jdbc:postgresql://localhost:5432/soundwrapped_db
SPRING_DATASOURCE_USERNAME=soundwrapped_user
SPRING_DATASOURCE_PASSWORD=supersecretpassword
SPRING_JPA_HIBERNATE_DDL_AUTO=update
SPRING_JPA_SHOW_SQL=false

# Google Knowledge Graph API Configuration
GOOGLE_KNOWLEDGE_GRAPH_API_KEY=your_google_api_key_here

# OpenAI API Configuration (optional, can use Groq instead)
OPENAI_API_KEY=your_openai_api_key_here

# Groq API Configuration (free, OpenAI-compatible alternative)
GROQ_API_KEY=your_groq_api_key_here

# SerpAPI Configuration (Web Search API)
SERPAPI_API_KEY=your_serpapi_api_key_here

# TheAudioDB API Configuration (Enhanced Artist Info)
THEAUDIODB_API_KEY=your_theaudiodb_api_key_here

# Last.fm API Configuration (Similar Artists & Scrobbling)
LASTFM_API_KEY=your_lastfm_api_key_here
LASTFM_API_SECRET=your_lastfm_api_secret_here